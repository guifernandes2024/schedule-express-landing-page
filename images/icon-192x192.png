      <text x="60" y="0" fill="#6b7280" font-family="Arial" font-size="8">Q</text>
      
      <text x="0" y="15" fill="#374151" font-family="Arial" font-size="8">1</text>
      <text x="15" y="15" fill="#374151" font-family="Arial" font-size="8">2</text>
      <text x="30" y="15" fill="#374151" font-family="Arial" font-size="8">3</text>
      <text x="45" y="15" fill="#374151" font-family="Arial" font-size="8">4</text>
      <text x="60" y="15" fill="#374151" font-family="Arial" font-size="8">5</text>
